(include "../scripts/library.hna")

(defun factorial0 (n)
  (funcall
   (lambda (n acc)
     (unless = n 1
             setq acc (self (1- n) (* n acc)))
     acc)
   n
   1))

defun factorial1 n
  funcall lambda pair
            noscope
              unless = car pair
                       1
                set-cdr pair
                        self cons 1- car pair
                                  * car pair
                                    cdr pair
              cdr pair
          cons n
               1

defun factorial2 n funcall lambda pair cdr cons unless = car pair 1 set-cdr pair self cons 1- car pair * car pair cdr
pair cdr pair cons n 1


(defun factorial3 (n)
  var acc 1
  (while > n 0
         setq acc (* n acc)
         setq n (1- n))
  acc)

defun factorial4 n
  noscope
    noscope
      var acc 1
      while > n 0
        noscope
          setq acc * n acc
          setq n 1- n
    acc

defun factorial5 n noscope noscope var acc 1 while > n 0 noscope setq acc * n acc setq n 1- n acc


defun factorial6 n 120


(()
 (defmacro p (c) (I)
           (list (list ' println
