(defmacro switch (constant &rest body) (L &rest -1 L)
  (if body
      (if (cdr body)
          (list ' if
                (list ' = constant (first body))
                (second body)
                (apply #self constant (cddr body)))
          (first body))
      ()))

(defmacro dosequence (variable sequence &rest body) (L I &rest 1 I)
