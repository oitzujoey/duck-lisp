           (list (' var) sequence-var sequence)
           (list (' var) return-var sequence)
           (list (' var) variable ())
           (list (' while) (list (' not) (list (' null?) sequence-var))
                 (list (' setq) variable (list (' car) sequence-var))
                 (apply #noscope
                        body)
                 (list ' setq sequence-var (list ' cdr sequence-var)))
           return-var))
